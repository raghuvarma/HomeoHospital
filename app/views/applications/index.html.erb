<!-- <p id="notice"><%# notice %></p> -->

<h1>Applications</h1>
<div class="create-form-link">
  <%= link_to 'New Application', new_application_path %>
</div>
<table class="table table-bordered table-hover">
  <thead>
    <tr>
      <th>Id</th>
      <th>Name</th>
      <th>Relation name</th>
      <th>Address</th>
      <th>Details</th>
      <!-- <th>Is doctor</th> -->
      <th colspan="2">Actions</th>
    </tr>
  </thead>

  <tbody>
    <% @applications.each do |application| %>
      <tr>
        <td><%= application.id %></td>
        <td><%= application.name %></td>
        <td><%= application.relation_name %></td>
        <td><%= application.address %></td>
        <td><%= application.details %></td>
        <!-- <td><%# application.is_doctor %></td> -->
        <td><%= link_to 'Show', application %></td>
        <td><%= link_to 'Edit', edit_application_path(application) %></td>
        <!-- <td><%# link_to 'Destroy', application, method: :delete, data: { confirm: 'Are you sure?' } %></td> -->
      </tr>
    <% end %>
  </tbody>
</table>

<div class="pagination_wrapper">
  <%= paginate @applications %>
</div>

<br>


